/* START GRID SETTINGS */

GridBackground {
	--grid-background-color: #282828;
	--line-color: #383838;
	--thick-line-color: #505050;
	--spacing: 10;
}

.edge{
	--edge-width: 10px !important;
}

/* END GRID SETTINGS */





/* Toolbar Start */

 .unity-toolbar.normal {
    height: 40px;
}

 .unity-toolbar.compact {
    height: 60px;
}
.normal .section{
	height: 40px;
}
.compact .section{
	height: 30px;
}

#save_btn, #load_btn, #import_btn, #export_btn, #translate_btn {
    -unity-text-align: lower-center;
    font-size: 10px;
    cursor: link;
    padding-bottom: 2px;
	width: 40px;
	align-items: center;
	align-self: stretch;
	padding-top: 0px;
	-unity-font-style: bold;
}

.compact #save_btn, .compact #load_btn, .compact #import_btn, .compact #export_btn, .compact #translate_btn{
	font-size: 0px;
	width: 30px;
}

.normal #translate_btn{
	width: 80px;
}

#setting_btn{

}
.normal #setting_btn .ToolbarIcon{
	width: 34px;
	height: 34px;
}
.compact #setting_btn .ToolbarIcon{
	width: 24px;
	height: 24px;
}

#localization_enum, #theme_enum {
    width: 100px;
    font-size: 10px;
    cursor: link;
}

.IconInToolbar{
	width: 26px;
	height: 26px;
	margin-top: 6px;
	margin-bottom: 6px;
	margin-left: 2px;
	margin-right: 2px;
}
.compact .IconInToolbar{
	width: 20px;
	height: 20px;
	margin-top: 5px;
	margin-bottom: 5px;
	margin-left: 2px;
	margin-right: 2px;
}
.normal .MatLogo{
	width: 40px;
	height: 40px;
}
.compact .MatLogo{
	width: 30px;
	height: 30px;
}

#localization_enum .unity-text-element, 
#theme_enum .unity-text-element {
    -unity-text-align: middle-center;
    padding-bottom: 0px;
}

.nameOfDialogueContainer {
    -unity-font-style: bold;
    min-width: 125px;
    -unity-text-align: middle-center;
    font-size: 10px;
}

#autosave_toogle {
    flex-direction: row-reverse;
    -unity-text-align: middle-left;
}

#autosave_toogle .unity-text-element {
    width: 75px;
    min-width: 75px;
    font-size: 10px;
}

#version_text {
    position: absolute;
    bottom: -8px;
    left: -8px;	
    font-size: 10px;
	background-color: #383838;
	padding: 16px;
	border-top-right-radius: 16px;
	border-width: 2px;
	border-color: #2d2d2d;
}
.ToolbarIcon{
	width: 24px;
	height: 24px;
	margin-top: 2px;
	margin-bottom: 2px;
	background-color: transparent;
	-unity-background-image-tint-color:  #d2d2d2;
}
.ToolbarBtnIcon{
}
.unity-toolbar-button,
.unity-toolbar-menu{
	border-width: 0;
}
.Indicator{
	border-width: 0;
	width: 40px;
	-unity-font-style: bold;
	padding: 0px;
	
}
.ErrorIndicator{
	color: #ff534a;
}
.WarningIndicator{
	color: #ffc107;
}
.Indicator .unity-help-box__icon{
	min-width: 16px;
	min-height: 16px;
	max-width: 16px;
	max-height: 16px;
}
/* Toolbar End */





/* VALIDATION */
  #ValidationContainer{
	  position:absolute !important;
	bottom: 38px;
	width: 200px;
	margin: 0px;
 }
  #ErrorContainer,
 #WarningContainer{
	background-color: #383838;
	padding: 8px;
	border-radius: 8px;
	-unity-font-style: bold;
	-font-size: 8px;

	border-color: #515151;
	border-width: 2px;

	width: auto;
	padding: 4px;
	margin: 0px;
	margin-top:5px;
 }
   #ErrorContainer .unity-help-box__icon,
 #WarningContainer .unity-help-box__icon{
	 min-height: 20px;
	 max-height: 20px;
	 min-width: 22px;
	 max-width:22px;
	 margin-right: 4px;
 }
   #ErrorContainer .unity-label,
 #WarningContainer .unity-label{
	 font-size: 10px;
 }
/* VALIDATION */





